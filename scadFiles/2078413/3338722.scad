    s2 = (x == size + border - 1 || !isPointVisible(x + 1, y)) ?
        [[p3i, p3o, p4i, p4o], [[0, 1, 2],[1, 3, 2]]] : [[], []],
    s3 = (y == -border || !isPointVisible(x, y - 1)) ?
