pcutoff = cutoff / 100;
size = sqrt(len(image));
hsize = size / 2;
qsize = (size + border * 2) / 4;
northpole = [0, 0, outer_radius] * 1;
wall_thickness = outer_radius - inner_radius;
cutoff_radius = sqrt(1 - pow(1 - pcutoff, 2));
thread_radius = cutoff_radius * outer_radius + thread_extra_thickness;
inner_cutoff_radius = cutoff_radius * inner_radius;
projector_thread_height = pitch * 2 + outer_radius * (1 - pcutoff);
dist_adapter_northpole = outer_radius * pcutoff;
adapter_body_length = dist_adapter_northpole - 2 * pitch;
adapter_body_inner_radius = cutoff_radius * outer_radius - wall_thickness + thread_extra_thickness;

module projector(preview = false) {
    translate([0, 0, projector_thread_height])
    rotate([180, 0, 0])
    difference() {
        if(preview) {
            difference() {
                sphere(r=outer_radius, $fn = getFn(preview));
                sphere(r=inner_radius, $fn = getFn(preview));
            }
        } else {
            create_projector();
        }
        translate([0, 0, outer_radius * (1 - pcutoff / 2)])
        cube([outer_radius * 2, outer_radius * 2, dist_adapter_northpole], center = true);
    }

    thread_projector(preview);
}

module create_projector() {
    data = [
        for (x = [-border: size + border - 1])
            let (rowdata = [for (y = [-border: size + border - 1])
                (isPointVisible(x, y)) ? drawPoint(x, y, getPoint(x, y)) : [[],[]]])
            addAllPointsAndFaces(rowdata, len(rowdata) - 1)
    ];
    polydata = addAllPointsAndFaces(data, len(data) - 1);
    polyhedron(points = polydata[0], faces = polydata[1]);
}

module thread_projector(preview = false) {
    difference() {
        cylinder(r = thread_radius, h = projector_thread_height, $fn = getFn(preview));
        translate([0, 0, projector_thread_height]) sphere(r = inner_radius + 0.1, $fn = getFn(preview));
        translate([0, 0, -pitch / 2]) thread(pitch, inner_cutoff_radius * 2, max(1, round(360 / getFn(preview))), 3, thread_tolerance, true);
    }
}

module lamp_adapter(preview = false) {
    difference() {
        union() {
            cylinder(r = thread_radius, h = adapter_body_length + bottom_thickness + cooler_height, $fn = getFn(preview));
            translate([0, 0, adapter_body_length + bottom_thickness + cooler_height + pitch / 3])
            thread(pitch, inner_cutoff_radius * 2,  max(1, round(360 / getFn(preview))), 1);
        }
        union() {
            hollow_lamp_body(preview);
            clear_light_path(preview);
            cable_holes();
            cooler(preview);
        }
    }
}

module hollow_lamp_body(preview = false) {
    translate([0, 0, bottom_thickness + cooler_height]) {
        cylinder(r = adapter_body_inner_radius, h = adapter_body_length - (wall_thickness + thread_extra_thickness) * 2 + 0.01, $fn = getFn(preview));
        translate([0, 0, adapter_body_length - (wall_thickness + thread_extra_thickness) * 2])
        cylinder(r1 = adapter_body_inner_radius, r2 = inner_cutoff_radius - inner_thread_thickness, h = wall_thickness + thread_extra_thickness, $fn = getFn(preview));
    }
    translate([0, 0, bottom_thickness + cooler_height + 0.01])
    cylinder(r = inner_cutoff_radius - inner_thread_thickness, h = dist_adapter_northpole, $fn = getFn(preview));
    }

module clear_light_path(preview = false) {
    translate([0, 0, bottom_thickness + cooler_height]) cylinder(r1 = 0, r2 = inner_cutoff_radius, h = dist_adapter_northpole, $fn = getFn(preview));
}

module cable_holes() {
    if (cable_hole_count >= 1) {
        translate([-cable_hole_position, 0, -0.01])
        cylinder(r = cable_hole_radius, h = bottom_thickness + cooler_height + 0.02, $fn = 30);
    }
    if (cable_hole_count >= 2) {
        translate([cable_hole_position, 0, -0.01])
        cylinder(r = cable_hole_radius, h = bottom_thickness + cooler_height + 0.02, $fn = 30);
    }
}

module cooler(preview = false) {
    translate([0, 0, bottom_thickness -0.01])
    cylinder(r = cooler_radius, h = cooler_height + 0.02, $fn = getFn(preview));
}

function getFn(preview) = preview ? 50 : (size * 4 * resolution_multiplier);

function drawPoint(x, y, v) =
let (
    xt = x - hsize,
    yt = y - hsize,
    p1 = project((xt - 0.5) / qsize, (yt - 0.5) / qsize),
    p2 = project((xt - 0.5) / qsize, (yt + 0.5) / qsize),
    p3 = project((xt + 0.5) / qsize, (yt + 0.5) / qsize),
    p4 = project((xt + 0.5) / qsize, (yt - 0.5) / qsize),
    p1i = intersectLineAndSphereAtCenter(northpole, p1 * inner_radius, inner_radius),
