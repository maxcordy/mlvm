/* [Cylinder] */

// Diameter at base of the object.
bottom_diameter = 184;

// For a lampshade shape, set top diameter less than base diameter. For a vase shape, set top diameter greater than base diameter.
top_diameter = 138;

// Total height of the object (including borders, if any).
total_height = 145;

// Height of bevelled borders at top and bottom of image. Set to 0 to disable borders.
border_height = 15;

// Borders are offset outward this far from image surface. Set to 0 to stretch top and bottom borders to top and bottom diameter circles.
border_offset = 0;

// Minimum thickness of cylinder walls. Actual thickness varies with image relief, like a lithophane. Set to 0 to disable. If disabled, object will be a solid model.
wall_thickness = 0;

// Set to less than 360 for a partial cylinder.
wrap_angle = 360;

/* [Hidden] */

image_width = 100;
image_height = 50;
max_width = image_width - 1;
max_height = image_height - 1;
face_height = total_height - (border_height * 2);
base_radius = bottom_diameter / 2;
top_radius = top_diameter / 2;
top_scale = 1 - top_radius/base_radius;
solid_radius = base_radius - relief_depth;
wall_radius = solid_radius - wall_thickness;

// polar to cartesian x,y
function pcx(radius, theta) = radius * sin(theta);
function pcy(radius, theta) = radius * cos(theta);

// value of image at x,y (max_width - x to preserve exterior left-right orientation)
function img(x, y) = image[(image_width * y) + (max_width - x)];

// surface radius for image x,y
function rad(x, y) = (relief_depth * img(x, y)) + solid_radius;

// taper scale at model z
function scl(z) = 1 - (z / total_height * top_scale);

// scaled image radius
function sir(x, y, z) = rad(x, y) * scl(z);

// rotation for image x
function rot(x) = x / max_width * wrap_angle;

// compute model z from image y
function y2z(y) = border_height + face_height * (1 - (y / max_height));

if (wall_thickness > 0) {
	difference() {
		union() {
			ImageSurface();
			Borders();
		}
		linear_extrude(height=total_height, scale=1-top_scale) circle(r=wall_radius, $fn=180);
	}
}
else {
	ImageSurface();
	Borders();
}

module ImageSurface() {
	for (y = [0:max_height-1]) {
		assign(top = y2z(y), bottom = y2z(y + 1)) {
			for (x = [0:max_width-1]) {
				ImageWedge(x, y, top, bottom);
			}
		}
	}
}

module ImageWedge(x, y, top, bottom) {
	Wedge(
		sir(x, y + 1, bottom),     // bottom left radius
		sir(x + 1, y + 1, bottom), // bottom right radius
		sir(x, y, top),            // top left radius
		sir(x + 1, y, top),        // top right radius
		rot(x),                    // left angle
		rot(x + 1),                // right angle
		top,
		bottom
	);
}

module Borders() {
	if (border_height > 0) {
		for (x = [0:max_width-1]) {
			TopBorder(x, y2z(0));
			BottomBorder(x, y2z(max_height));
		}
	}
}

module TopBorder(x, bottom) {
	Wedge(
		sir(x, 0, bottom),
		sir(x + 1, 0, bottom),
		(border_offset == 0 ? top_radius : border_offset + sir(x, 0, total_height)),
		(border_offset == 0 ? top_radius : border_offset + sir(x + 1, 0, total_height)),
		rot(x),
		rot(x + 1),
		total_height,
		bottom
	);
}

module BottomBorder(x, top) {
	Wedge(
		(border_offset == 0 ? base_radius : border_offset + sir(x, max_height, 0)),
		(border_offset == 0 ? base_radius : border_offset + sir(x + 1, max_height, 0)),
		sir(x, max_height, top),
		sir(x + 1, max_height, top),
		rot(x),
		rot(x + 1),
		top,
		0
	);
}

module Wedge(bl, br, tl, tr, la, ra, top, bottom) {
	polyhedron(
		points=[                               // viewed facing wedge outer surface
			[0, 0, bottom],                     // 0 - bottom inner axis
			[pcx(bl, la), pcy(bl, la), bottom], // 1 - bottom left
			[pcx(br, ra), pcy(br, ra), bottom], // 2 - bottom righ
			[0, 0, top],                        // 3 - top inner axis
			[pcx(tl, la), pcy(tl, la), top],    // 4 - top left
			[pcx(tr, ra), pcy(tr, ra), top]     // 5 - top right
		],
		triangles=[//triangles= is now deprecated, but Thingiverse doesn't know faces= :-P
			[0, 1, 2], // 0 - bottom
			[1, 4, 2], // 1 - outer/bottom
			[4, 5, 2], // 2 - outer/top
			[5, 0, 2], // 3 - right/bottom
			[0, 5, 3], // 4 - right/top
			[0, 4, 1], // 5 - left/bottom
			[4, 0, 3], // 6 - left/top
			[3, 5, 4]  // 7 - top
		]
	);
}
