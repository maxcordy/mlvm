use <write/Write.scad> // use <Write.scad>
// preview[view:south, tilt:top]


//
// CUSTOMIZING

/* [Text] */ //Choose to add text
//Do you want to add text?
show_text="yes"; //[yes,no]
//Text Font
text_font="knewave.dxf";//[knewave.dxf:Knewave, orbitron.dxf:Orbitron,Letters.dxf:Letter, BlackRose.dxf:BlackRose, braille.dxf:Braille]
//What do you want the text to say at front side?
textone="Dr.";
texttwo="MAHESH KUMAR";
//default at position [0,0,0]
//text left to right
//XposText = -9; //[-5:5]
//y to come in front
//YposText = 1.5; //[-50:50]
//text bottom to top
//ZposText = 2; //[1:20]

//Text size(height)

//whether embossed(outwards) or engraved(inwards) text
text_allignment="engraved"; //[embossed,engraved]


//Spacing between adjacent letters
//text_spacing=1.5;

//default uploaded image height 10
//image_height = 0; //[-50:50]
  
// Load a 100x100 pixel image. (images will be automatically stretched to fit) Simple, high contrast images like logos work best.
//image_file = "image-surface.dat"; // [image_surface:100x100]


//
// ASSEMBLE BUILDING
if(text_allignment=="embossed"){
union(){

		deskname(1);
	if (show_text=="yes") {
			translate([8,0,40]){

			color("salmon")
			rotate(60,[0,1,0])   // rotate around the x axis
		rotate(90,[0,0,1])  // rotate around the y axis
			//rotate(90,[0,1,0])  // rotate around the y axis
	//	rotate(180,[1,0,0])  // rotate around the x axis
//			rotate(180,[0,1,0])   // rotate around the x axis
//			rotate(90,[0,0,1])  // rotate around the z axis
			write(textone,t=1.6,bold=0,h=9,font=text_font,space=1.3, center = true);		

	 
				}//end-of-text-translate

translate([16.6,-1,25]){

			color("salmon")
			rotate(62,[0,1,0])   // rotate around the x axis
		rotate(90,[0,0,1])  // rotate around the y axis
			//rotate(90,[0,1,0])  // rotate around the y axis
	//	rotate(180,[1,0,0])  // rotate around the x axis
//			rotate(180,[0,1,0])   // rotate around the x axis
//			rotate(90,[0,0,1])  // rotate around the z axis
			write(texttwo,t=1.6,bold=0,h=9,font=text_font,space=1.3, center = true);		

	 
				}//end-of-text-translate
			

		}
}//end-of-text-union
}

if(text_allignment=="engraved"){
render()
difference(){

		deskname(1);
	if (show_text=="yes") {
	
	translate([8,0,40]){

			color("salmon")
			rotate(60,[0,1,0])   // rotate around the x axis
		rotate(90,[0,0,1])  // rotate around the y axis
			//rotate(90,[0,1,0])  // rotate around the y axis
	//	rotate(180,[1,0,0])  // rotate around the x axis
//			rotate(180,[0,1,0])   // rotate around the x axis
//			rotate(90,[0,0,1])  // rotate around the z axis
			write(textone,t=1.6,bold=0,h=9,font="Letters.dxf",space=1.3, center = true);		

	 
				}//end-of-text-translate

translate([16,-1,25]){

			color("salmon")
			rotate(62,[0,1,0])   // rotate around the x axis
		rotate(90,[0,0,1])  // rotate around the y axis
			//rotate(90,[0,1,0])  // rotate around the y axis
	//	rotate(180,[1,0,0])  // rotate around the x axis
//			rotate(180,[0,1,0])   // rotate around the x axis
//			rotate(90,[0,0,1])  // rotate around the z axis
			write(texttwo,t=1.6,bold=0,h=9,font="Letters.dxf",space=1.3, center = true);		

	 
				}//end-of-text-translate
			

		}
}//end-of-text-union
}
